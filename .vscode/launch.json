{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Flutter + Backend",
      "configurations": ["Flutter", "start-node"]
    }
  ],
  "configurations": [
    {
      "name": "Flutter",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "start-node",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/lib/backend/index.js",
      "console": "integratedTerminal"
    }
  ]
}
